                 </div>
               </div>
               <div class="row form-group">
                 <label class="control-label col-md-4">Confirm Password</label>
                 <div class="col-md-8">
                  <!-- <input class="form-control" type="password" placeholder="*************"> -->
                   <%= f.password_field :confirm_password, class: "form-control", placeholder: "*************" %>
                 </div>
               </div>
