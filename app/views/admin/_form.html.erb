                      <%= comission_form.text_field :to_wallet, class: "form-control", placeholder: "wallet to wallet" %><br><!-- <input class="form-control" type="text" placeholder="0.00%"> -->
                     </div>
                     <div class="col-md-8">
                      <%= comission_form.text_field :to_bank, class: "form-control", placeholder: "wallet to bank" %><br><!-- <input class="form-control" type="text" placeholder="0.00%"> -->
